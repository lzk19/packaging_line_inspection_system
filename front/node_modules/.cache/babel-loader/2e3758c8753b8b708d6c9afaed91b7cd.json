AAA,QAtFA,sBAsFA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,QAAA,CACA,kBADA,EAEA,IAFA,EAGA;AACA,kBAAA,iBAAA,EAAA,IADA;AAEA,kBAAA,gBAAA,EAAA,IAFA;AAGA,kBAAA,IAAA,EAAA;AAHA,iBAHA,EAQA,KARA,CAQA,UAAA,GAAA;AAAA,yBAAA,GAAA;AAAA,iBARA,CADA;;AAAA;AACA,gBAAA,cADA;;AAAA,sBAUA,cAAA,IAAA,SAVA;AAAA;AAAA;AAAA;;AAAA,kDAWA,MAAA,CAAA,QAAA,CAAA,IAAA,CAAA,OAAA,CAXA;;AAAA;AAaA,gBAAA,GAbA,GAaA,MAAA,CAAA,iBAAA,CAAA,GAAA,CAAA,UAAA,CAAA;AAAA,yBAAA,CAAA,CAAA,EAAA;AAAA,iBAAA,CAbA,EAaA;;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,iBAAA,EAAA,GAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,sBAAA,GAAA,CAAA,IAAA,KAAA,KAAA,EAAA;AACA,oBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AACA,oBAAA,MAAA,CAAA,IAAA;AACA,mBAHA,MAGA;AACA,oBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA;AACA;AACA,iBAPA;;AAdA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAsBA,KA5GA;AA8GA,IAAA,KA9GA,mBA8GA;AACA,WAAA,QAAA,GAAA,EAAA;AACA,WAAA,KAAA,GAAA,EAAA;AACA,WAAA,OAAA,GAAA,EAAA;AACA,WAAA,IAAA;AACA,KAnHA;AAqHA,IAAA,mBArHA,+BAqHA,OArHA,EAqHA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA,WAAA,OAAA,GAAA,OAAA;AACA,WAAA,IAAA;AACA,KAzHA;AA0HA,IAAA,GA1HA,iBA0HA;AACA,MAAA,MAAA,CAAA,IAAA,kBAAA,QAAA;AACA;AA5HA;AAvBA,CAAA","sourcesContent":["<template>\n  <div>\n\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\n      <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\n      <el-breadcrumb-item>用户管理</el-breadcrumb-item>\n      <el-breadcrumb-item>员工管理</el-breadcrumb-item>\n    </el-breadcrumb>\n\n    <el-card>\n\n      <!-- el-row:gutter设置两个el-col之间的间隙 -->\n      <el-row :gutter=\"25\">\n        <!-- 搜索区域 -->\n        <!-- el-col:span设置列宽 -->\n        <el-col :span=\"8\">\n          <el-input placeholder=\"请输入搜索内容\"\n                    clearable\n                    @clear=\"reset\"\n                    v-model=\"username\">\n            <el-button slot=\"append\"\n                       icon=\"el-icon-search\"\n                       @click=\"load\">\n            </el-button>\n          </el-input>\n\n        </el-col>\n        \n          <el-button type=\"primary\"\n                     @click=\"handleAdd\" >添加用户 <i class=\"el-icon-circle-plus-outline\"></i></el-button>\n\n          <el-button type=\"primary\" @click=\"exp\" class=\"ml-5\" style=\"margin-left:12px\">导出 <i class=\"el-icon-top\"></i></el-button>\n        \n        <el-button type=\"danger\"  style=\"margin-left:12px\" @click=\"delBatch\">批量删除 <i class=\"el-icon-remove-outline\"></i></el-button>\n      </el-row>\n\n\n    <el-table :data=\"tableData\" border stripe :header-cell-class-name=\"'headerBg'\"  @selection-change=\"handleSelectionChange\">\n      <el-table-column type=\"selection\" width=\"55\"></el-table-column>\n      <!-- 把列的type设置成index类型就成为了索引列 -->\n\n        <el-table-column type=\"index\"\n                         width=\"50\"\n                         align=\"center\">\n          <template slot-scope=\"scope\">\n            <span>{{\n              (pageNum - 1) * pageSize + (scope.$index + 1)\n            }}</span>\n          </template>\n        </el-table-column>\n\n      <el-table-column prop=\"username\" label=\"用户名\" width=\"120\"></el-table-column>\n      <el-table-column prop=\"name\" label=\"姓名\" width=\"120\"></el-table-column>\n      <el-table-column prop=\"sex\" label=\"性别\" width=\"80\"></el-table-column>\n      <el-table-column prop=\"phone\" label=\"电话\"></el-table-column>\n      <el-table-column prop=\"email\" label=\"邮箱\"></el-table-column>\n      <el-table-column prop=\"address\" label=\"地址\"></el-table-column>\n      <el-table-column prop=\"role\" label=\"角色\" width=\"130px\" align=\"center\">\n        <template slot-scope=\"scope\">\n          <el-tag type=\"primary\" v-if=\"scope.row.role === 'ROLE_ADMIN'\">管理员</el-tag>\n          <el-tag type=\"warning\" v-if=\"scope.row.role === 'ROLE_TEACHER'\">线长</el-tag>\n          <el-tag type=\"success\" v-if=\"scope.row.role === 'ROLE_STUDENT'\">员工</el-tag>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\"  width=\"160\" align=\"center\">\n        <template slot-scope=\"scope\">\n\n          <!-- 修改按钮 -->\n            <el-tooltip content=\"修改员工信息\"\n                        placement=\"top\"\n                        effect=\"light\">\n              <el-button type=\"primary\"\n                         icon=\"el-icon-edit\"\n                         plain\n                         round\n                         size=\"mini\"\n                          @click=\"handleEdit(scope.row)\"></el-button>\n            </el-tooltip>\n            <!-- 删除按钮 -->\n            <el-tooltip content=\"删除员工\"\n                        placement=\"top\"\n                        effect=\"light\">\n              <el-button type=\"danger\"\n                         icon=\"el-icon-delete\"\n                         round\n                         plain\n                         size=\"mini\"\n                         @click=\"del(scope.row.id)\"></el-button>\n            </el-tooltip>\n\n        </template>\n      </el-table-column>\n    </el-table>\n    \n      <el-pagination\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"pageNum\"\n          :page-size=\"pageSize\"\n          layout=\"total, prev, pager, next, jumper\"\n          :total=\"total\">\n      </el-pagination>\n    </el-card>\n\n    <el-dialog title=\"用户信息\" :visible.sync=\"dialogFormVisible\" width=\"30%\" >\n      <el-form label-width=\"80px\" size=\"small\">\n        <el-form-item label=\"用户名\">\n          <el-input v-model=\"form.username\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n\n        <el-form-item label=\"名字\">\n          <el-input v-model=\"form.name\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n\n        <el-form-item label=\"性别\">\n          <el-input v-model=\"form.sex\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n\n        <el-form-item label=\"电话\">\n          <el-input v-model=\"form.phone\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n\n        <el-form-item label=\"邮箱\">\n          <el-input v-model=\"form.email\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n\n        <el-form-item label=\"地址\">\n          <el-input v-model=\"form.address\" autocomplete=\"off\"></el-input>\n        </el-form-item>\n\n        <el-form-item label=\"角色\">\n          <el-select clearable v-model=\"form.role\" placeholder=\"请选择角色\" style=\"width: 100%\">\n            <el-option v-for=\"item in roles\" :key=\"item.name\" :label=\"item.name\" :value=\"item.flag\"></el-option>\n          </el-select>\n        </el-form-item>\n        \n        \n        \n        \n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogFormVisible = false\">取 消</el-button>\n        <el-button type=\"primary\" @click=\"save\">确 定</el-button>\n      </div>\n    </el-dialog>\n\n    \n  </div>\n</template>\n\n<script>\nimport {serverIp} from \"../../public/config\";\n\nexport default {\n  name: \"User\",\n  data() {\n    return {\n      serverIp: serverIp,\n      tableData: [],\n      total: 0,\n      pageNum: 1,\n      pageSize: 7,\n      username: \"\",\n      email: \"\",\n      address: \"\",\n      form: {},\n      dialogFormVisible: false,\n      multipleSelection: [],\n      roles: [],\n      vis: false,\n      stuVis: false\n    }\n  },\n  created() {\n    this.load()\n  },\n  methods: {\n    lookCourse(courses) {\n      this.courses = courses\n      this.vis = true\n    },\n    lookStuCourse(stuCourses) {\n      this.stuCourses = stuCourses\n      this.stuVis = true\n    },\n    load() {\n      this.request.get(\"/user/page\", {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          username: this.username,\n          email: this.email,\n          address: this.address,\n        }\n      }).then(res => {\n\n        this.tableData = res.data.records\n        this.total = res.data.total\n\n      })\n\n      this.request.get(\"/role\").then(res => {\n        this.roles = res.data\n      })\n    },\n    save() {\n      this.request.post(\"/user\", this.form).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"保存成功\")\n          this.dialogFormVisible = false\n          this.load()\n        } else {\n          this.$message.error(\"保存失败\")\n        }\n      })\n    },\n\n    handleAdd() {\n      this.dialogFormVisible = true\n      this.form = {}\n    },\n\n    handleEdit(row) {\n      this.form = JSON.parse(JSON.stringify(row))\n      this.dialogFormVisible = true\n    },\n\n\n    async del(id) {\n\n      const confirmResult = await this.$confirm(\n        \"此操作将永久删除用户，是否继续？\",\n        \"提示\",\n        {\n          confirmButtonText: \"确定\",\n          cancelButtonText: \"取消\",\n          type: \"warning\",\n        }\n      ).catch((err) => err); //把结果信息放进了confirmResult中\n      if (confirmResult != \"confirm\") {\n        return this.$message.info(\"已取消删除\");\n      }\n      this.request.delete(\"/user/\" + id).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"删除成功\")\n          this.load()\n        } else {\n          this.$message.error(\"删除失败\")\n        }\n      })\n    },\n\n\n\n\n\n    handleSelectionChange(val) {\n      console.log(val)\n      this.multipleSelection = val\n    },\n\n\n    async delBatch() {\n      const confirmResult1 = await this.$confirm(\n        \"此操作将永久删除用户，是否继续？\",\n        \"提示\",\n        {\n          confirmButtonText: \"确定\",\n          cancelButtonText: \"取消\",\n          type: \"warning\",\n        }\n      ).catch((err) => err); //把结果信息放进了confirmResult中\n      if (confirmResult1 != \"confirm\") {\n        return this.$message.info(\"已取消删除\");\n      }\n      let ids = this.multipleSelection.map(v => v.id)  // [{}, {}, {}] => [1,2,3]\n      this.request.post(\"/user/del/batch\", ids).then(res => {\n        if (res.code === '200') {\n          this.$message.success(\"批量删除成功\")\n          this.load()\n        } else {\n          this.$message.error(\"批量删除失败\")\n        }\n      })\n    },\n\n    reset() {\n      this.username = \"\"\n      this.email = \"\"\n      this.address = \"\"\n      this.load()\n    },\n\n    handleCurrentChange(pageNum) {\n      console.log(pageNum)\n      this.pageNum = pageNum\n      this.load()\n    },\n    exp() {\n      window.open(`http://${serverIp}:9090/user/export`)\n    },\n\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n.el-table {\n  margin-top: 15px;\n  font-size: 15px;\n}\n</style>\n"],"sourceRoot":"src/views"}]}